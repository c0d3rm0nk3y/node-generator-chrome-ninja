{
  "blacklist": [ "regenerator" ],
  "optional":  [
    "es7.objectRestSpread",
    "es7.classProperties",
    "es7.decorators"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-transform"
      ],
      "extra": {
        "react-transform": {
          "transforms": [{
            "transform": "livereactload/babel-transform",
            "imports": ["react"]
          }]
        }
      }
    }
  }
}
